#include <stdio.h>
#include <stdlib.h>

int main(int __attribute__ ((unused)) argc, char *argv[])
{
	/** 
	 * Get the second argument of the main function
	 * Iterate through the array of pointers starting from the second
	 * Then print all the arguments using %s in printf function
	 * Stop when you reach a null pointer
	 */

	int i;

	for (i = 0; argv[i] != NULL; i++)
	{
		printf("%s\n", argv[i]);
	}
}
